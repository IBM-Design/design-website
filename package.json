    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "7.1.1",
    "cross-env": "^5.2.0",
    "fs-extra": "^7.0.1",
    "gatsby-remark-static-images": "^1.0.0",
    "gh-pages": "^1.2.0",
    "html-loader": "^0.5.5",
    "node-sass": "^4.10.0",
    "prettier": "^1.15.3",
    "rimraf": "^3.0.2",
    "sass-loader": "^7.1.0",
    "storybook-readme": "^3.3.0"
  },
  "eslintConfig": {
    "globals": {
      "__PATH_PREFIX__": true
    }
  },
  "prettier": {
    "jsxBracketSameLine": true,
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "es5"
  }
